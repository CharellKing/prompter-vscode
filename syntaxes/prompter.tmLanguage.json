{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Prompter",
    "patterns": [
        {
            "include": "#keywords"
        },
        {
            "include": "#strings"
        },
        {
            "include": "#comments"
        },
        {
            "include": "#numbers"
        },
        {
            "include": "#operators"
        }
    ],
    "repository": {
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.control.prompter",
                    "match": "\\b(if|else|for|while|do|switch|case|break|continue|return|function|var|let|const|class|import|export|from|as|default)\\b"
                },
                {
                    "name": "keyword.operator.prompter",
                    "match": "\\b(new|typeof|instanceof|in|of)\\b"
                },
                {
                    "name": "storage.type.prompter",
                    "match": "\\b(boolean|number|string|object|undefined|null)\\b"
                }
            ]
        },
        "strings": {
            "patterns": [
                {
                    "name": "string.quoted.double.prompter",
                    "begin": "\"",
                    "end": "\"",
                    "patterns": [
                        {
                            "name": "constant.character.escape.prompter",
                            "match": "\\\\."
                        }
                    ]
                },
                {
                    "name": "string.quoted.single.prompter",
                    "begin": "'",
                    "end": "'",
                    "patterns": [
                        {
                            "name": "constant.character.escape.prompter",
                            "match": "\\\\."
                        }
                    ]
                },
                {
                    "name": "string.template.prompter",
                    "begin": "`",
                    "end": "`",
                    "patterns": [
                        {
                            "name": "meta.embedded.expression.prompter",
                            "begin": "\\$\\{",
                            "end": "\\}",
                            "patterns": [
                                {
                                    "include": "$self"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        "comments": {
            "patterns": [
                {
                    "name": "comment.line.double-slash.prompter",
                    "match": "//.*$"
                },
                {
                    "name": "comment.block.prompter",
                    "begin": "/\\*",
                    "end": "\\*/"
                }
            ]
        },
        "numbers": {
            "patterns": [
                {
                    "name": "constant.numeric.prompter",
                    "match": "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b"
                }
            ]
        },
        "operators": {
            "patterns": [
                {
                    "name": "keyword.operator.arithmetic.prompter",
                    "match": "\\+|\\-|\\*|\\/|%"
                },
                {
                    "name": "keyword.operator.comparison.prompter",
                    "match": "==|!=|===|!==|<|>|<=|>="
                },
                {
                    "name": "keyword.operator.logical.prompter",
                    "match": "&&|\\|\\||!"
                },
                {
                    "name": "keyword.operator.assignment.prompter",
                    "match": "=|\\+=|\\-=|\\*=|\\/=|%="
                }
            ]
        }
    },
    "scopeName": "source.prompter"
}